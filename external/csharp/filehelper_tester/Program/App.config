<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <add key="Debug" value="False"/>
    <add key="RetryInterval" value="250"/>
  	<add key="Retries" value="10"/>
  	<!-- NOTE: currently all retries are used in ReadContents loop -->
    <add key="HoldInterval" value="10000"/>
    <add key="Datafile" value="c:\temp\data.txt"/>
  </appSettings>
</configuration>
